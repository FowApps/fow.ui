    "types": "lib/esm/index.d.ts",
    "sideEffects": false,
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{jsx,js,ts,tsx}": "eslint src/** && stylelint src/**"
    },
