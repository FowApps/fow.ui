        "build-types": "yarn tsc -d --emitDeclarationOnly --outDir types",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "pre-commit-lint": "eslint src/**",
        "lint:css": "stylelint 'src/**'",
